{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/react"  
      },
      {
        "src": "backend/api/index.js",
        "use": "@vercel/node"  
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/backend/api/index.js"  
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"  
      }
    ]
  }
  